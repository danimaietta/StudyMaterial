{
  "name": "testing-react",
  "version": "1.0.0",
  "description": "Testing project using Jest with react.",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "NODE_ENV='production' webpack",
    "build-for-windows": "SET NODE_ENV='production' && webpack",
    "test": "jest"
  },
  "babel": {
